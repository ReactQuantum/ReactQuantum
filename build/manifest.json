{
  "manifest_version": 2,
  "name": "React Quantum",
  "version": "0.1.0",
  "devtools_page": "devtools.html",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "backgroundScript.js"
    ],
    "persistant": true
  },
  "web_accessible_resources": [
    "inject.js"
  ],
  "externally_connectable": {
    "ids": [
      "*"
    ]
  },
  "icons": {
    "128": "ReactQuantumLogo_WebStoreIcon.png"
  }
}